import { ElementRef } from '@angular/core';
import { SwitchBase } from '../../core/components/switch/switch';
export declare class MbscSwitch extends SwitchBase {
    vInput: ElementRef;
    vHandle: ElementRef;
    vHandleCont: ElementRef;
    checked: any;
    color: 'primary' | 'secondary' | 'success' | 'danger' | 'warning' | 'info' | 'dark' | 'light';
    description: string;
    disabled: boolean;
    id: string;
    label: string;
    position: 'start' | 'end';
    readonly: boolean;
    _id: string;
    private _check;
    registerOnChange(fn: any): void;
    registerOnTouched(fn: any): void;
    setDisabledState(disabled: boolean): void;
    writeValue(val: any): void;
    protected _change(checked: boolean): void;
    protected _mounted(): void;
}
